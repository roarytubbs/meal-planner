# Required for Prisma/Postgres backend.
DATABASE_URL=

# Server-only API key used by /api/places/* routes.
# Do not commit real values to the repository.
GOOGLE_PLACES_API_KEY=

# Optional: recipe provider key used by /api/import-recipe/providers/spoonacular/* routes.
SPOONACULAR_API_KEY=

# Optional: server-side Target cart connector used by /api/shopping/cart-session.
# TARGET_CART_SESSION_ENDPOINT should point to your provider/partner endpoint that
# creates a checkout cart session from ingredient items.
TARGET_CART_SESSION_ENDPOINT=
TARGET_CART_SESSION_API_KEY=
TARGET_CART_TIMEOUT_MS=10000
TARGET_CART_CACHE_TTL_MS=60000
SHOPPING_CART_RATE_LIMIT_WINDOW_MS=60000
SHOPPING_CART_RATE_LIMIT_MAX_REQUESTS=10
